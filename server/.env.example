# Port number on which the server should run (e.g., 3000)
PORT=somePort

# Primary database URL (used by Prisma and the backend)
DATABASE_URL=your_database_url_here

# Direct database connection URL (for migrations and direct queries)
DIRECT_URL=your_direct_database_url_here

# Secret key for signing JWT tokens (e.g., a long random string)
JWT_SECRET=your_jwt_secret_here